<%= simple_form_for(@flat) do |f| %>
  <%= f.input :title %>
  <%= f.input :address %>
  <%= f.input :city%>
  <%= f.input :monthly_price%>
  <%= f.input :capacity%>
  <%= f.input :type_of_flat%>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
